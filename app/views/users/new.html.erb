<h1><%= t 'users.new' %></h1>

<%= form_for @user do |form| -%>
  <%= form.error_messages %>

  <%= form.label :email %>
  <%= form.text_field :email %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.label :confirmation %>
  <%= form.password_field :confirmation %>


  <%= form.submit t('user.create_button') %>
<% end -%>
